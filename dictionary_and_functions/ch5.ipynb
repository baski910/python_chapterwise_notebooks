   "source": [
    "# consuming rest api and get a dictionary from a json object\n",
    "import requests\n",
    "import json\n",
    "\n",
    "r = requests.get(\"https://reqres.in/api/users?page=2\")\n",
    "\n",
    "if r.status_code == 200:\n",
    "    d1 = json.loads(r.text)\n",
    "    print(d1)\n",
    "else:\n",
    "    print(\"could not get result from rest end point\")"
   ]
  }
